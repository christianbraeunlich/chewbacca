{
	"name": "chewbacca",
	"displayName": "Chewbacca",
	"publisher": "otter",
	"description": "My Web Extension Template",
	"author": {
		"name": "Christian Br√§unlich",
		"email": "sundowneffect@gmail.com",
		"url": "https://github.com/christianbraeunlich"
	},
	"version": "0.0.1",
	"icon": "images/logo.png",
	"engines": {
		"vscode": "^1.67.0"
	},
	"license": "MIT",
	"homepage": "https://github.com/christianbraeunlich/chewbacca",
	"repository": {
		"type": "git",
		"url": "https://github.com/christianbraeunlich/chewbacca"
	},
	"bugs": {
		"url": "https://github.com/christianbraeunlich/chewbacca/issues"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:chewbacca.helloWorld"
	],
	"browser": "./dist/web/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "chewbacca.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
		"pretest": "npm run compile-web",
		"vscode:prepublish": "npm run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/mocha": "^9.1.1",
		"eslint": "^8.14.0",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-web": "^0.0.24",
		"ts-loader": "^9.2.8",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2",
		"@types/webpack-env": "^1.16.4",
		"assert": "^2.0.0",
		"process": "^0.11.10"
	}
}